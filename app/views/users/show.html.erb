<h1><%= @user.name %>'s Page</h1>
<%= 'Admin' if @user.admin%>
<div>
  <%= render partial: 'user', locals: {user: @user, parties: @user.parties, characters: @user.characters}%>
</div>
<div class='navigation-links'>
  <%= link_to 'Back to Users', users_path %>
  <% if @user == current_user %>
    | <%= link_to 'Edit User', edit_user_path, method: :get%> </div>
  <div>
    <%= button_to 'Delete Profile', user_path, method: :delete%>
    <%= button_to 'Sign Out', signout_path, method: :post %>
  <% end %>
</div>
